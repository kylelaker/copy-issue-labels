{
  "name": "copy-issue-labels",
  "version": "1.0.3",
  "private": true,
  "description": "Copies labels to a pull request from its linked issues (and their parents)",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt -e '*.test.*'",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kylelaker/copy-issue-labels.git"
  },
  "author": "Kyle Laker <kyle@laker.email>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^5.0.3",
    "@octokit/graphql": "^5.0.1",
    "@octokit/graphql-schema": "^12.4.1"
  },
  "devDependencies": {
    "@types/jest": "^29.0.1",
    "@types/node": "^18.7.16",
    "@typescript-eslint/parser": "^5.39.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.23.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^27.0.4",
    "jest": "^29.0.3",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.0",
    "typescript": "^4.8.3"
  }
}
